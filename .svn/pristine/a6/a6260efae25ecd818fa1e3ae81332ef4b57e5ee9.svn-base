.content{
    padding-top: 20px;
}
.list-tab{
    margin-top: 30px;
}
.list-item:first-child{
    border: none;
}
.list-item:first-child>div{
    border-left:none;
}
.list-item-info {
    height: 100px;
    position: relative;
    border-top: 1px solid #ddd;
}
.list-item>div{
    flex-direction:column;
}
.list-item>div.welfare-name {
    flex-direction:row;
    flex: 1;
    min-width: 140px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.mw100 {
    min-width: 110px;
}
.mw80 {
    min-width: 100px;
}
.mw100 span{
    display: block;
}
.opreat {
    width: 15%;
    min-width: 190px;
}
.welfare-name img,.order-name img {
    width: 60px;
    height: 60px;
    margin-right: 5px;
}
.lh12{
    line-height: 12px;
}
.op-item {
    margin-right: 10px;
    color: #58acfb;
}
.ecode-box {
    display: none;
    width: 334px;
    height: 250px;
    border: 1px solid #ddd;
    background: #fff;
    z-index: 999;
    position: absolute;
    right: 124px;
    top: 58px;
    text-align: left;
    color: #101010;
    padding: 20px 20px 16px;
}
.ecode-box div {
    position: relative;
}
.ecode-box input[type='text'] {
    height: 40px;
    width: 100%;
    border-radius: 4px;
    border: 1px solid #F96E4A;
    margin-bottom: 20px;
}
.ecode-box input[type='button'] {
    height: 40px;
    background: #F96E4A;
    position: absolute;
    right: 0;
    top: 0;
    border-radius: 4px;
    border: none;
    outline: none;
    color: #fff;
}
.ecode-box .code-img {
    display: inline-block;
    width: 90px;
    height: 90px;
    border: 1px solid #bbb;
    padding: 5px;
    margin-right: 10px;
    vertical-align: middle;
}
.ecode-box span {
    color: #41B1E0;
    line-height: 90px;
}
.number-time{
    width: 100%;
    height: 40px;
    display: flex;
    align-items: center;
    color: #999;
    padding-left: 12px;
    font-size:12px;
}
.order-number{
    margin-right:80px;
}
.phone{
    color: #a4a4a4;
    font-size:12px;
}
.money{
    font-size:16px;
    color: #f86f4d;
    font-weight: bold;
}
.oreder-info-sum{
    width: 100%;
    height: 40px;
    padding-right: 20px;
    text-align: right;
    line-height: 40px;
    color: #808080;
    font-size: 12px;
    border-right: 1px solid #ddd;
    border-bottom: 1px solid #ddd;
    border-left: 1px solid #ddd;
}
.fact-money{
    margin: 0 20px 0 10px;
}
.big-text{
    font-style:normal;
    font-size:18px;
}
.text-orange{
    font-style:normal;
    color: #f86f4d;
}
.text-red{
    color: #f33e46;
}
.list-content-foot{
    margin-top:60px;
    border-top: 1px solid #ddd;
}
.order-head{
    margin-right:-20px;
    margin-bottom: 36px;
}
.order-head>div{
    float: left;
    font-size:16px;
    color: #a9a9a9;
}
.order-number span{
    color: #000;
}
.order-head>div.order-btn{
    float: right;
    margin-right: -14px;
}
.order-title{
    width: 100%;
    height: 20px;
    border-left: 4px solid #f86f4d;
    font-size: 14px;
    color: #040606;
    padding-left: 10px;
    line-height: 20px;
    margin-bottom: 10px;
    margin-top: 14px;
}
.info{
    width: 100%;
    height: 46px;
    border-top: 1px solid #ddd;
    line-height: 46px;
    padding-left: 14px;
}
.info-item{
    margin-right:50px;
    color: #999;
}
.info-item i{
    font-style: normal;
    color: #000;
}