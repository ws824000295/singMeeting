.order-detail {
  width: 100%;
  height: 100%;
  background: #f4f4f4;
  padding-bottom: 1.3rem;
}

.order-detail-header {
  box-sizing: border-box;
  width: 100%;
  height: auto;
  overflow: hidden;
  background: #fff;
  color: #666;
  font-size: .37rem;
  padding: 0.426rem;
}

.order-num {
  margin-top: .27rem;
}

.shop-goods {
  box-sizing: border-box;
  width: 100%;
  height: 3.7rem;
  background: #fff;
  color: #666;
  font-size: .37rem;
  margin-top: .37rem;
  padding: 0.426rem;
}

.status {
  color: #f8614a;
  float: right;
}

.goods-info {
  border-top: 1px solid #d1d0d1;
  margin-top: 0.426rem;
  display: flex;
  padding-top: .4rem;
}

.goods-img {
  width: 2.96rem;
  height: 1.6rem;
  margin-right: .27rem;
}

.goods-img img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.goods-flex {
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: space-around;
  justify-content: space-between;
}

.goods-num {
  width: 1.1rem;
  color: #f8614a;
  text-align: right;
}

.customer-info {
  width: 100%;
  margin-top: .4rem;
  height: auto;
  overflow: hidden;
  background: #fff;
  padding: .426rem;
  box-sizing: border-box;
  font-size: .37rem;
  color: #666;
}

.customer-name {
  margin-right: .8rem;
}

.customer-addr {
  margin-top: .24rem;
}

.order-info {
  border-top: 1px solid #d1d0d1;
  margin-top: .37rem;
  padding-top: .37rem;
  line-height: .53rem;
}

.order-info-text {
  display: inline-block;
  width: 1.87rem;
}

.order-info-data {
  color: #000;
}

.factPay, .factPay .order-info-data {
  border-top: 1px solid #d1d0d1;
  margin-top: .37rem;
  padding-top: .37rem;
  line-height: .53rem;
  color: #f60000;
}
.factPay .order-info-data{
	border-top: 0;
}
.connect {
  width: 100%;
  height: auto;
  overflow: hidden;
  margin-top: .37rem;
  background: #fff;
  padding-top: .4rem;
  padding-bottom: .4rem;
}

.connect div {
  width: 4.1rem;
  height: .96rem;
  border: 1px solid #666;
  color: #666;
  font-size: .37rem;
  margin: 0 auto;
  clear: both;
  text-align: center;
  line-height: .96rem;
}

.iocn-call {
  width: .56rem;
  height: .56rem;
  vertical-align: middle;
  margin-right: .32rem;
}

.send {
  width: 100%;
  height: 1.3rem;
  background: #f8614a;
  color: #fff;
  font-size: .45rem;
  text-align: center;
  line-height: 1.3rem;
  position: fixed;
  left: 0;
  bottom: 0;
}

.logistics {
  width: 100%;
  height: auto;
  color: #666;
  font-size: .37rem;
  background: #fff;
  padding: .43rem;
  line-height: .53rem;
  margin-top: .43rem;
}

.send-alert, .checkLogistics-alert {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  background: rgba(0, 0, 0, 0.4);
  z-index: 999;
  display: none;
}

.close {
  width: .4rem;
  height: .4rem;
  position: absolute;
  top: .267rem;
  right: .267rem;
}

.checkLogistics-alert-content {
  width: 8.5rem;
  height: 13rem;
  overflow-y: scroll;
  background: #fff;
  border-radius: .2rem;
  margin: 0 auto;
  margin-top: 1rem;
  font-size: .35rem;
  color: #797979;
  position: relative;
}

.logistics-company {
    width: 8.5rem;
    height: auto;
    border-bottom: 1px solid #cbcecb;
    padding: .67rem;
    font-size: .37rem;
    color: #808080;
    line-height: .53rem;
    padding-bottom: .37rem;
    position: fixed;
    z-index: 999;
    background: #fff;
    border-radius: .2rem .2rem 0 0;
}
#myContent {
    position: absolute;
    top: 1.57rem;
}
.order-number {
  color: #000;
}

.order-tel {
  color: #0090ff;
}

.logistics-info {
  padding: .43rem;
}
.logistics-status{
  color: #f97a00;
}
.logistics-info-item {
    display: flex;
    margin-bottom: .8rem;
    align-items: baseline;
    padding-left: 0.5rem;
}

.logistics-info-item:first-child div {
  color: #f97a00;
}

.data {
  width: 1.95rem;
  margin-right: .33rem;
}

.icon {
  width:0;
}

.status-addr {
  flex: 1;
}

.addr {
  margin-top: .267rem;
}
.kuaidiPhone{
  display: none;
}

.checkLogistics-alert-content::-webkit-scrollbar {display:none}

.checkLogistics-alert-content .close{
  position: fixed;
  top: 1.33rem;
  right: 1rem;
  z-index: 9999;
}
#welfareName{
	overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}