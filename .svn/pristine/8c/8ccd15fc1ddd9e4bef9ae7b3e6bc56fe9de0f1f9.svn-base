* {
  margin: 0;
  padding: 0;
  text-decoration: none;
  box-sizing: border-box;
}

html {
  width: 100%;
}

body {
  height: 100%;
  display: flex;
  background: white;
}

div.side {
  width: 14%;
  background-color: #322c58;
  list-style-type: none;
}

.show2 {
  min-width: 1160px;
}

div.showHead {
  width: 100%;
  height: 64px;
  position: relative;
  background-color: #fe565c;
}

.showHeadInfo {
  width: 205px;
  height: 40px;
  position: absolute;
  right: 4%;
  top: 50%;
  display: flex;
  transform: translateY(-50%);
}

.showHeadbell {
  width: 30%;
  height: 20px;
  margin-top: 20px;
  transform: translateY(-50%);
  padding-left: 3%;
  position: relative;
  border-left: 1px solid rgba(255, 255, 255, 0.8);
}

.showHeadbell img {
  display: block;
  margin-left: 25%;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
}

.showHeadDown {
  width: 66%;
  height: 40px;
  line-height: 37px;
  border-left: 1px solid rgba(255, 255, 255, 0.8);
  font-size: 12px;
  color: white;
  padding-left: 10%;
}

.showHeadDown span {
  vertical-align: middle;
}

.showHeadDown img {
  vertical-align: middle;
  margin-left: 3%;
}

.showPosition {
  height: 50px;
  padding: 15px;
}

.showPosition span {
  color: #f86f4d;
}

.showCenter {
  padding: 0 !important;
  margin: 0 auto !important;
  float: none;
}

.span4 {
  height: 38px !important;
  float: left !important;
}

.list_list {
  display: flex;
  border-bottom: 1px solid #e1e1e1;
  margin-left: 0 !important;
  margin-top: 60px;
}

.list_list li {
  width: 8%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  border-top-left-radius: 5px;
  border-top-right-radius: 5px;
  margin-right: 10px;
  cursor: pointer;
  background: #f1f1f1;
  border-top: 1px solid #e1e1e1;
  border-left: 1px solid #e1e1e1;
  border-right: 1px solid #e1e1e1;
  transition: 0.15s;
}

li.list_active {
  height: 41px;
  background: white;
  margin-bottom: -1px !important;
  border-top: 1px solid #f86f4d;
  border-left: 1px solid #f86f4d;
  border-right: 1px solid #f86f4d;
  color: #f86f4d;
}

.contentList > .row div {
  /* background:black; */
}

.contentList > .row {
  height: 60px;
  line-height: 60px;
  background: #f1f1f1;
  font-size: 14px;
}

input[type="checkbox"] {
  width: 16px;
  vertical-align: middle;
  height: 16px;
}

.home_list {
  margin-left: 0 !important;
}

.home_left,
.home_right {
