 .speaker1 {
 margin: -0.2rem 0 0 0.05rem;
    width: .4rem;
    height: .4rem;
    border: 2px solid;   
    border-radius: 50%;
    opacity: 1;
    border-color: transparent #fff transparent transparent;
}
.speaker2 {
    position: absolute;
    top: 50%;
    left: 50%;
    width: .25rem;
    height: .25rem;
    margin: -.22rem 0 0 0;
    border: 2px solid;
    border-radius: 50%;
    opacity: 1;
   	border-color: transparent #fff transparent transparent;
}
.speaker3 {
    position: absolute;
    top: 50%;
    left: 50%;
    width: 2px;
    height: 2px;
    margin: -0.1rem 0 0 0.06rem;;
    border: 2px solid;    
    border-radius: 50%;
    border-color: transparent #fff transparent transparent;
}
.speech_bar .speaker1,.speech_bar .speaker2,.speech_bar .speaker3{
    border-color: transparent #fff transparent transparent;
}
.pressed .speaker1{
    -webkit-animation: speaker1 1.2s ease-in-out infinite;
     -webkit-animation-play-state: paused;
    }
.pressed .speaker2 {
    -webkit-animation: speaker2 1.2s ease-in-out infinite;
     -webkit-animation-play-state: paused; 
   }
   @-webkit-keyframes speaker1{
    0%{
        opacity: 0; 
    }
    70%,100%{
        opacity: 1;
    }
}
@-webkit-keyframes speaker2{
    0%{
        opacity: 0; 
    }
    20%,100%{
        opacity: 1;
    }
}
