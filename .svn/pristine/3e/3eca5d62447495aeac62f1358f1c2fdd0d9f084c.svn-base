	position:relative;
}
.audio-resoure span{
    width: .7rem;
    height: .7rem;
    position: absolute;
    left: .2rem;
    top: .1rem;
    background: url(../../images/songli/process.png) no-repeat;
    background-size: 3.5rem;
    background-position:0rem -.15rem;
}
.audio-resoure span.bofang{
	-webkit-animation: bofang 1.5s steps(1) infinite;
}
.audio-resoure b{
	position:absolute;
	width:1rem;
	right:-1.2rem;
	color:#999;
	height:.9rem;
	line-height:.9rem;
	font-weight:normal;
	font-size:.35rem;
}
@-webkit-keyframes bofang{
    25% {
       background-position:-1.19rem -.17rem;
    }
    50% {
       background-position:-1.99rem -.15rem;
    }
    75% {
       background-position:-2.77rem -.15rem;
    }
    100% {
       background-position:0  -.15rem;
    }
}
@keyframes bofang{
    25% {
       background-position:-1.19rem -.17rem;
    }
    50% {
       background-position:-1.99rem -.15rem;
    }
    75% {
       background-position:-2.77rem -.15rem;
    }
    100% {
       background-position:0  -.15rem;
    }
}
