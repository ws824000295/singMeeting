*{
	border: 0;
	margin: 0;
	padding: 0;
}
body{
	background: #f2f2f2;
}
.banner{
	width: 100%;
	height: auto;
	position: relative;
}
.banner-info{
	width: 100%;
	height: 4.27rem;
	background: url('../../../images/fuli/banner.png') no-repeat center;
	background-size: 100%;
	padding: .4rem;
	display: flex;
}
.banner_et{
	width: 100%;
	height: auto;
	position: relative;
}
.banner-info_et{
	width: 100%;
	height: 4.27rem;
	background: url('../../../images/fuli/banner_et.png') no-repeat center;
	background-size: 100%;
	padding: .4rem;
	display: flex;
}
.my-avatar{
	width: 1.6rem;
	height: 1.6rem;
	border-radius: 50%;
	overflow: hidden;
}
.my-avatar img{
	width: 100%;
	height: 100%;
	object-fit: cover;
	border-radius: 50%;
	border: 0.075rem solid #ffecaa;
}
.my-info{
	flex: 1;
	margin-left: 0.267rem;
	padding-top: 0.3rem;
	color: #fff;
	position: relative;
}
.my-name{
	font-size: 0.45rem;
}
.icon-edit{
	display: inline-block;
	width: 0.43rem;
	height: 0.4rem;
	background: url('../../../images/fuli/edit.png') no-repeat center;
	background-size: 100%;
	margin-left: .4rem;
}
.star-level{
	margin-bottom: 0.04rem;
	margin-top: 0.04rem;
}
.icon-start{
	display: inline-block;
	width: 0.36rem;
	height: 0.4rem;
	background: url('../../../../images/fuli/me_xing.png') no-repeat center;
	background-size: 100%;
	margin-right: 0.1rem;
}
.score-tips{
	font-size: 0.27rem;
}
.score-tips a{
	text-decoration: none;
	color: #fff;
	border: 1px solid #fff;
	border-radius: 0.3rem;
	padding: 0.12rem 0.16rem;
}
.ecode{
	position: absolute;
	top: 0.4rem;
	right: 0rem;
}
.icon-ecode{
	display: block;
	width: .453rem;
	height: .43rem;
	background: url('../../../images/fuli/ecode.png') no-repeat center;
	background-size: 100%;
	margin: 0 auto;
}
.banner-position{
	width: 9.35rem;
	height: 1.73rem;
	background: url('../../../images/fuli/center-banner.png') no-repeat center;
	background-size: 100%;
	position: absolute;
	left: 50%;
	margin-left: -4.7rem;
	bottom: -0.8rem;
	display: flex;
}
.quick{
	width: 3rem;
	height: 1rem;
	background: url('../../../images/fuli/quick.png') no-repeat center;
	background-size: 100%;
	position: absolute;
	left: 1.78rem;
	top: -0.8rem;
}
.banner-position-lf{
	position: relative;
    width: 2.5rem;
}
.cion{
	width: 0.88rem;
	object-fit: cover;
	vertical-align: middle;
	margin-top: 0.53rem;
	margin-left: 0.26rem;
}
.coin-box{
	float: right;
    text-align: center;
    padding-top: -0.6rem;
    position: absolute;
    margin-top: -1rem;
    margin-left: 1rem;
}
.coin-text{
	display: block;
	font-size: 0.32rem;
	color: #666;
	margin-left: 0.3rem;
}
.coin-num,.fubishop{
	font-size: 0.56rem;
	color: #f6805a;
}
.banner-position-rg{
	margin-top: 0.4rem;
	flex: 1;
	padding-left: 0.8rem;
	display: flex;
}
.position-rg-box{
	flex: 1;
}
.center-cart{
	width: 1.6rem;
	margin-right: 0.53rem;
}
.shop-tips{
	color: #a6a3a3;
	margin-top: -0.2rem;
	font-size: 0.3rem;
}
.icon-shop{
	width: 0.93rem;
	height: 0.84rem;
	margin-right: 0.23rem;
}
.play{
	display: block;
	width: 9.33rem;
	max-height:2rem;
	margin: 0 auto;
	margin-top: 1rem;
}
.necessary,.assist{
	width: 9.33rem;
	height: auto;
	overflow: hidden;
	background: #fff;
	margin: 0 auto;
	margin-top: 0.23rem;
	border-radius: .51rem;
}
.personal-title{
	color: #393939;
	font-size: 0.35rem;
	padding: 0.3rem .4rem .2rem;
	border-bottom: 1px solid #ededed;
}
.personal-list{
	padding-top: .4rem;
}
.necessary-item{
	width: 20%;
	float: left;
	text-align: center;
}
.necessary-icon{
	display: block;
	width: 0.64rem;
	height: .6rem;
	margin: 0 auto;
	margin-bottom: 0.17rem;
}
.icon-order{
	background: url('../../../images/fuli/icon-order.png') no-repeat center;
	background-size: 100%;
}
.icon-discount{
	background: url('../../../images/fuli/icon-discount.png') no-repeat center;
	background-size: 100%;
}
.icon-fubi{
	background: url('../../../images/fuli/icon-fubi.png') no-repeat center;
	background-size: 100%;
}
.icon-fubishop{
	background: url('../../../images/fuli/icon-fubishop.png') no-repeat center;
	background-size: 100%;
}
.icon-superior{
	background: url('../../../images/fuli/icon-superior.png') no-repeat center;
	background-size: 100%;
}
.necessary-item span{
	color: #666;
	font-size: .3rem;
}
.news{
	clear: both;
	padding: .4rem;
}
.news-content{
	width: 100%;
	height: 1.83rem;
	background: #f2f2f2;
	border-radius: .35rem;
	padding: .27rem;
}
.news-title{
	color: #666;
	font-size: .27rem;
}
.news-item{
	display: flex;
	align-items: center;
	justify-content:flex-start;
	font-size: .27rem;
	margin-top: 0.17rem;
}
.news-avatar{
	width: .83rem;
	height: .83rem;
	border-radius: .25rem;
	overflow: hidden;
	float: left;
}
.news-avatar img{
	width: 100%;
	height: 100%;
}
.news-center{
	float: left;
	flex: 1;
	color: #666;
	padding-left: 0.3rem;
}
.news-top span,.news-bottom span{
	color: #fb560a;
}
.news-right{
	width: 1.73rem;
	color: #acabab;
	text-align: right;
}
.assist{
	padding-bottom: .8rem;
	margin-bottom:2rem;
}
.assist-list{
	width: 100%;
	height: auto;
	text-align: center;
}
.assist-item{
	width: 25%;
	float: left;
	margin-top: .6rem;
}
.assist-icon{
	display: block;
	width: 0.93rem;
	height: 0.84rem;
	margin: 0 auto;
}
.icon-sign{
	background: url('../../../images/fuli/icon-sign.png') no-repeat center;
	background-size: 100%;
}
.icon-inviting{
	background: url('../../../images/fuli/icon-inviting.png') no-repeat center;
	background-size: 100%;
}
.icon-myshare{
	background: url('../../../images/fuli/icon-myshare.png') no-repeat center;
	background-size: 100%;
}

.icon-focus{
	background: url('../../../images/fuli/icon-focus.png') no-repeat center;
	background-size: 100%;
}
.icon-group{
	background: url('../../../images/fuli/icon-group.png') no-repeat center;
	background-size: 100%;
}
.icon-msg{
	background: url('../../../images/fuli/icon-msg.png') no-repeat center;
	background-size: 100%;
}
.icon-view{
	background: url('../../../images/fuli/icon-view.png') no-repeat center;
	background-size: 100%;
}
.icon-help{
	background: url('../../../images/fuli/icon-help.png') no-repeat center;
	background-size: 100%;
}
/* 我的福星 */
.superior-alert{
	width: 100vw;
	height: 100vh;
	position: fixed;
	top: 0;
	bottom: 0;
	background: rgba(0,0,0,0.5);
	z-index: 999;
}
.superior-alert-content{
	width: 7.5rem;
	height: 10.45rem;
	position: absolute;
	top: 2.36rem;
	left: 50%;
	margin-left: -3.75rem;
	background: #fff;
	border-radius: 0.15rem;
}
.superior-close{
	width: 1.12rem;
	height: 1.12rem;
	background: url('../../../images/fuli/less-close.png') no-repeat center;
	background-size: 100%;
	border-radius: 50%;
	position: absolute;
	top: -0.33rem;
	right: -0.27rem;
}
.superior-bg{
	width: 100%;
}
.superior-avatar{
	text-align: center;
	margin-top: -1.12rem;
}
.superior-avatar img{
	width: 1.73rem;
	border-radius: 50%;
	border: 0.08rem solid #fab6b4;
}
.name-sex{
	margin-top: .4rem;
	text-align: center;
}
.superior-name{
	font-size: .4rem;
	color: #626161;
}
.icon-sex{
	display: inline-block;
	width: .36rem;
	height: .36rem;
	margin-left: 0.28rem;
}
.icon-sex.female{
	background: url('../../../images/fuli/female.png') no-repeat center;
	background-size: 100%;
}
.icon-sex.male{
	background: url('../../../images/fuli/male.png') no-repeat center;
	background-size: 100%;
}
.time-count{
	margin-top: .74rem;
	text-align: center;
	height: auto;
	overflow: hidden;
}
.time,.count{
	width: 50%;
	float: left;
}
.time{
	border-right: 1px solid #cdcbcb;
}
.time-count-text{
	font-size: .3rem;
	color: #626161;
}
.time-count-value{
	font-size: .28rem;
	color: #fb560a;
}
.iwant{
	clear: both;
	display: block;
	width: 6.4rem;
	height: 0.92rem;
	background: #f45552;
	color: #fff;
	margin: 0 auto;
	text-align: center;
	line-height: 0.92rem;
	margin-top: 1.2rem;
	border-radius: 0.18rem;
}
.star-level {
    margin-bottom: 0.04rem;
    margin-top: 0.04rem;
}

.icon-start {
    display: inline-block;
    width: 0.36rem;
    height: 0.4rem;
    background: url(../../../images/fuli/me_xing.png) no-repeat center;
    background-size: 100%;
    margin-right: 0.1rem;
}
