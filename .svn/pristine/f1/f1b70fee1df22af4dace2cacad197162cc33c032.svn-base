.fl_addcount{
	background: #f3f3f3;
    padding: 0;
    height: auto;
}
.less-tips{
	width: 100%;
	height: 39px;
	color: #fff;
	font-size: 16px;
	line-height: 39px;
	background: #ffbc5e;
}
.icon-exclamation{
	display: inline-block;
	width: 24px;
	height: 24px;
	background: url('../../../images/fuli/dataOverview/exclamation.png') no-repeat center;
	background-size: 100%;
	vertical-align: middle;
	margin-left: 20px;
    margin-right: 15px;
    margin-top: -3px;
}
.less-tips a{
	color: #FFF;
	text-decoration: underline;
}
.company-info{
	padding: 0 14px;
    padding-top: 40px;
}
.company-name{
	font-size: 28px;
	color: #101010;
}
.company-date{
    color: #999999;
    font-size: 14px;
}
.company-operate{
	float: right;
}
.company-operate input{
	display: inline-block;
	width: 128px;
    height: 45px;
	border: 1px solid #ddd;
    font-size: 14px;
    text-align: center;
    line-height: 20px;
    background: #fcfcfc;
}
.design,.visit{
	float: left;
}
.design{
	margin-right: 20px;
}
.overview-title{
	font-size: 18px;
	color: #101010;
	margin-top: 20px;
	padding: 0 14px;
}
.account {
	background: #f3f3f3;
    height: 95px;
    padding: 10px;
    width: 98%;
    margin: 0 auto;
    margin-bottom: 20px;
}
.account div {
    float: left;
    width: 25%;
    height: 100%;
    text-align: center;
    border-right: 1px solid #ccc;
    line-height: 30px;
}
.account div:last-child{
	border-right: 0;
}
.account div span{
	color: #333;
    font-weight: 700;
    font-style: normal;
    font-size: 23px;
}
.panorama,.todday-data,.often{
	height: auto;
	overflow: hidden;
}
.panorama div{
	float: left;
    width: 19%;
    height: 100px;
    font-size: 14px;
    text-align: center;
    line-height: 20px;
    background: #fff;
    margin-right: .8%;
    display: flex;
    align-items: center;
    justify-content: space-between;
    padding: 0 20px;
}
.panorama div:first-child{
	margin-left: .8%;
}
.panorama div:last-child{
	margin-right: 0;
}
.panorama-icon{
	width: 40px;
	height: 40px;
}
.icon-company-count{
	background: url('../../../images/fuli/dataOverview/icon-company-count.png') no-repeat center;
	background-size: 100%;
}
.icon-stream{
	background: url('../../../images/fuli/dataOverview/icon-stream.png') no-repeat center;
	background-size: 100%;
}
.icon-pen{
	background: url('../../../images/fuli/dataOverview/icon-pen.png') no-repeat center;
	background-size: 100%;
}
.icon-commission{
	background: url('../../../images/fuli/dataOverview/icon-commission.png') no-repeat center;
	background-size: 100%;
}
.icon-fuxing{
	background: url('../../../images/fuli/dataOverview/icon-fuxing.png') no-repeat center;
	background-size: 100%;
}
.panorama div p span{
	display: block;
}
.todday-data div{
	float: left;
	width: 19%;
	margin-right: .8%;
	height: 100px;
	text-align: center;
	color: #fff;
    border-radius: 10px;
}
.todday-data div:first-child{
	margin-left: .8%;
}
.todday-data div:last-child{
	margin-right: 0;
}
.todday-data div p:first-child{
	border-bottom: 2px solid #fff;
    padding: 5px;
}
.today-look{
	background: #619ce7;
}
.today-trade{
	background: #fd6894;
}
.today-share{
	background: #25bcb9;
}
.today-order{
	background: #de60f9;
}
.today-fuxing{
	background: #faac55;
}
.total-box{
	width: 100%;
	height: auto;
	background: #fff;
    margin-top: 30px;
    overflow: hidden;
    padding-bottom: 30px;
    position: relative;
}
.top100{
	width: 60%;
	height: 343px;
	float:left;
}
.stream-echart{
	width: 95%;
	height: 290px;
}
.often div{
	float: left;
	width: 19%;
	height: 47px;
	background: #f3f3f3;
	margin-right: .8%;
	display: flex;
	align-items: center;
	padding: 0 20px;
    cursor: pointer;
}
.often div:first-child{
	margin-left: .8%;
}
.often div:last-child{
	margin-right: 0;
}
.often div i{
	display: block;
	width: 31px;
	height: 31px;
	margin-right: 15px;
}
.icon-fuli{
	background: url('../../../images/fuli/dataOverview/icon-fuli.png') no-repeat center;
	background-size: 100%;
}
.icon-earn{
	background: url('../../../images/fuli/dataOverview/icon-earn.png') no-repeat center;
	background-size: 100%;
}
.icon-fulicompany{
	background: url('../../../images/fuli/dataOverview/icon-fulicompany.png') no-repeat center;
	background-size: 100%;
}
.icon-coupon{
	background: url('../../../images/fuli/dataOverview/icon-coupon.png') no-repeat center;
	background-size: 100%;
}
.icon-help{
	background: url('../../../images/fuli/dataOverview/icon-help.png') no-repeat center;
	background-size: 100%;
}
.tui-list{
	width: 100%;
	height: auto;
	overflow: hidden;
	background: #fafafa;
	border-left: 1px solid #ededed;
	border-right: 1px solid #ededed;
}
.tui-item{
	display: flex;
	height: 70px;
	align-items: center;
	border-bottom: 1px solid #EDEDED;
	text-align: center;
}
.tui-item span{
	flex: 1;
}
.tui-money{
	color: #e51c23;
}


.top-box{
    height: auto;
    margin-left: 30px;
    overflow: hidden;
    clear: both;
}
.top100-tab{
	float: left;
    width: 287px;
    height: 122px;
    border-color: rgba(37, 188, 185, 1);
    border-radius: 12px 12px 12px 12px;
    font-size: 14.0px;
    padding: 17px;
    border-width: 1px;
    border-style: solid;
    text-align: center;
    line-height: 20px;
    margin-top: 80px;
}
.top-type .icon{
	display: inline-block;
    width: 30px;
    height: 14px;
    background-color: rgba(221, 221, 221, 1);
    border-radius: 21px 21px 21px 21px;    
    vertical-align: middle;
    margin-right: 5px;
}
.icon.green-icon{	
    background-color: rgba(37, 188, 185, 1);
}

.green{
	color:rgba(37, 188, 185, 1);
}
.top-type{
	display:inline-block;
	width:50%;
	margin: 5px 0;
    cursor: pointer;
}
.stream-box{
    height: auto;
    overflow: hidden;
    position: relative;
}
.stream-tab{
    position: absolute;
    top: 0;
    right: 200px;
    width: 80px;
    height: 30px;
    border: 1px solid rgba(37, 188, 185, 1);
    border-radius: 25px;
}
.stream-tab span{
    float: left;
    width: 50%;    
    height: 100%;        
    text-align: center;
    line-height: 30px;
    color: rgba(37, 188, 185, 1);
	background: #fff;
    cursor: pointer;
}
.stream-tab span:first-child{
    border-radius: 25px 0 0 25px;
}
.stream-tab span:last-child{
	border-radius: 0 25px 25px 0;
}

.stream-type.stream-green{
	background: rgba(37, 188, 185, 1);
	color: #fff;
}