.color-box{
    padding:10px;
    width:570px;
    border:1px solid #ccc;
}
.color-box ul{
    width:550px;
}
.color-box ul li{
   float:left;
   width:20%;
   margin-bottom:10px;
   position:relative;
}
.color-box ul li:nth-child(6),.color-box ul li:nth-child(7),.color-box ul li:nth-child(8),.color-box ul li:nth-child(9),.color-box ul li:nth-child(10){
    margin-bottom:0;
}
.color-box ul li a{
    display:block;
    width:90%;
    margin:auto;
    height:100px;
    background:#000;
}
.color-box ul li:nth-child(1) a{
    background:#62b358;
}
.color-box ul li:nth-child(2) a{
    background:#2c9f66;
}
.color-box ul li:nth-child(3) a{
    background:#509fc9;
}
.color-box ul li:nth-child(4) a{
    background:#5885cf;
}
.color-box ul li:nth-child(5) a{
    background:#9162c1;
}
.color-box ul li:nth-child(6) a{
    background:#d19b44;
}
.color-box ul li:nth-child(7) a{
    background:#e5b138;
}
.color-box ul li:nth-child(8) a{
    background:#ef913c;
}
.color-box ul li:nth-child(9) a{
    background:#dd6448;
}
.color-box ul li:nth-child(10) a{
    background:#cd463c;
}
.color-box ul li span{
    display:block;
    width:90%;
    height:90%;
    position:absolute;
    top:3%;
    right:10%;
    z-index:99999;
}
.color-box ul li.on span{
    background:url("../../images/coupon/check_color.png") no-repeat right bottom;
    background-size:40px 40px;
}
#wrapper {
    width: 980px;
    margin:0;
    width: auto;
}

#container {
    border: 1px solid #dadada;
    color: #838383;
    font-size: 12px;
    background-color: #FFF;
}

#uploader .queueList,#uploader1 .queueList {
    margin: 20px;
}.element-invisible {
    position: absolute !important;
    clip: rect(1px 1px 1px 1px); /* IE6, IE7 */
    clip: rect(1px,1px,1px,1px);
}

#uploader .placeholder,#uploader1 .placeholder {
    border: 3px dashed #e6e6e6;
    min-height: 200px;
    padding-top: 55px;
    text-align: center;
    color: #cccccc;
    font-size: 18px;
    position: relative;
}

 .webuploader-pick {
    font-size: 18px;
    background: #00b7ee;
    border-radius: 3px;
    line-height: 44px;
    padding: 0 30px;
    color: #fff;
    display: inline-block;
    margin: 20px auto;
    width: 160px;
    height: 45px;
    position: relative;
    cursor: pointer;
    box-shadow: 0 1px 1px rgba(0, 0, 0, 0.1);
}
 .webuploader-pick input[type=file]{
 	position: absolute;
 	top: 0;
 	bottom: 0;
 	left: 0;
 	opacity: 0;
 	right: 0;
 }
#uploader .placeholder .webuploader-pick-hover,#uploader1 .placeholder .webuploader-pick-hover {
    background: #00a2d4;
}

#uploader .placeholder .flashTip,#uploader1 .placeholder .flashTip {
    color: #666666;
    font-size: 12px;
    position: absolute;
    width: 100%;
    text-align: center;
    bottom: 20px;
}


.webuploader-element-invisible {
    position: absolute !important;
    clip: rect(1px 1px 1px 1px);
    clip: rect(1px,1px,1px,1px);
}.artcile_img {
    color: #a94442;
    font-size: 14px;
    margin-top: 10px;
}
.clearfix{
	clear: both;
}
.wxeditor {
    width: 100%;
