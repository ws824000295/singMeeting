table.table_ td{padding:12px 10px;border-color:#e6ebed;border-style:solid;border-width:1px;text-align:center}
.layui-table-sort .layui-table-sort-desc {
    bottom: 4px;
    border-bottom: none;
    border-top-style: solid;
    border-top-color: #b2b2b2;
}
.layui-edge {
    display: inline-block;
    width: 0;
    height: 0;
    border-width: 6px;
    border-style: dashed;
    border-color: transparent;
    overflow: hidden;
    position: absolute;
    left: 5px;
    border-width: 5px;
    vertical-align: middle;
}
.layui-table-sort {
    width: 10px;
    height: 20px;
    margin-left: 5px;
    cursor: pointer!important;
    position: relative;
    display: inline-block;
    vertical-align: -5px;
}.layui-table-sort .layui-table-sort-asc {
    top: 4px;
    border-top: none;
    border-bottom-style: solid;
    border-bottom-color: #b2b2b2;
}
