	body {
		background: #f5f5f5;
	}
	.drillUp {
		width: 100%;
		height: 100%;
	}
	.header {
		width: 100%;
		height: 1.0666rem;
		background: #fff;
		margin-top: 0.2666rem;
		/*margin-bottom: 0.2666rem;*/
	}
	.header .header-left {
		font-size: 0.32rem;
		width: 100%;
		padding-left: 0.2666rem;
		display: flex;
		/*justify-content: center;*/
		/*align-items: center;*/
		line-height: 1.0666rem;
	}
	.header .header-left .amount {
		color: #ff190c;
	}
	.amount-info {
		width: 100%;
	}
	.amount-info ul {
		width: 100%;
		padding: 0rem 0.2666rem;
		margin-top: 0.2666rem;
	}
	.amount-info li {
		margin-bottom: 0.2666rem;
		display: block;
		background: #fff;
		widows: 100%;
		height: 1.4666rem;
		line-height: 1.4666rem;
		border-radius: 0.1rem;
		color: #000;
	}
	.amount-info li img {
		width: 0.64rem;
		height: 0.64rem;
		margin-left: 0.3466rem;
		/*float: left;*/
	}
	.amount-info li .coin {
		font-size: 0.3466rem;
		margin-left: 0.4rem;
		/*float: left;*/
	}
	.amount-info li .money {
		font-size: .37333rem;
		float: right;
		width: 2.16rem;
		color: #ff190c;
		border-left: 0.0133rem solid #f3f3f3;
		text-align: center;
	}
	.amount-info .active {
		background: #ff534c;
		border-radius: 0.1rem;
		color: #fff!important;
	}
	.amount-info .actived {
		color: #fff;
	}
	.amount-item input{
		border: none;
	}
	.test {
		color: #999999;
		margin-top: 0.2666rem;
		font-size: 0.2666rem;
		padding-left: 0.2666rem;
	}
	.footer {
		width: 8rem;
		height: 1.0666rem;
		line-height: 1.0666rem;
		background: #ff534c;
		border-radius: 0.12rem;
		color: #fff;
		font-size: 0.48rem;
		margin: 0 auto;
		text-align: center;
		margin-top: 0.5333rem;
	}