}
.button .make{
  width: 1.6rem;
  height: 0.53rem;
  background: url('../../images/view/fbi.png') no-repeat;
  background-size: 1.6rem 0.53rem;
  margin-right: 0.2rem;
  color: #b157f6;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.button .nolimited{
  width: 1.2rem;
  height: 0.53rem;
  background: url('../../images/view/diq.png') no-repeat;
  background-size: 1.2rem 0.53rem;
  color: #f45752;
}
.levelShow{
