#addressSelectWrapper {
	display:none;
    position: fixed;
    top: 0.9rem;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0.5);
}
#addressSelect {
    z-index: 999;
    position: absolute;
    width: 100%;
    height: 70%;
    overflow: hidden;
    background-color: #fff;
}
#addressSelect .tip {
    position: relative;
    height: 0.53rem;
    line-height: 0.53rem;
    width: 100%;
    border-bottom: 1px solid #dedede;
}
#addressSelect .tip h3 {
    width: 100%;
    height:0.53rem;
    line-height: 0.53rem;
    text-align: center;
    font-size: 0.3rem;
    font-weight: normal;
    color: #666;
}
#addressSelect .tip #cancel {
    position: absolute;
    top: 0;
    right: 0;
    width: 0.52rem;
    height: 0.52rem;
    color: #666;
    border: none;
    background: transparent url("images/cancel.png") no-repeat center;
    cursor: pointer;
}
#addressSelect #address {
    width: 100%;
    height: 100%;
}
/*琚�変腑鐨勫湴鍧�*/
.selected-address {
    width: 100%;
    height: 0.63rem;
    line-height: 0.63rem;
    border-bottom: 1px solid #ededed;
}
.selected-address:before {
    content: "";
    display: table;
}
.selected-address:after {
    content: "";
    display: table;
    clear: both;
}
.selected-address li {
    float: left;
    height: 0.63rem;
    line-height: 0.63rem;
    text-align: center;
    width: 25%;
    padding-right: .3em;
    padding-left: .3em;
    color: #333;
    border-bottom: 1px solid transparent;
    font-size: 0.32rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    cursor: pointer;
}
.selected-address li.active {
    border-bottom-color: #3da5fe;
    color: #3da5fe;
}
/*鍚勭骇鍦板潃鍒楄〃*/
.address-content {
    position: relative;
    width: 100%;
    height: 100%;
    overflow: hidden;
    /*鍔犺浇涓�*/
}
.address-content ul {
    display: block;
    width: 100%;
    height: 100%;
    overflow-y: auto;
    padding-top: 0.1rem;
    padding-bottom: 1.73rem;
}
.address-content ul li {
    height: 0.8rem;
    line-height: 0.8rem;
    padding-left: 1em;
    font-size: 0.3rem;
    width: 100%;
    overflow-x: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: #333;
    cursor: pointer;
}
.address-content ul li.active {
    background-color: #3da5ef;
    color: #fff;
}
.address-content .loading {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    padding: 14px 14px 14px 52px;
    background: rgba(0, 0, 0, 0.5) url("images/loading.gif") no-repeat left 14px center / 24px 24px;
    color: #fff;
    border-radius: 0.08rem;
    font-size: 0.32rem;
}
#clickme {
  width: 1.6rem;
  height: 1.6rem;
  position: fixed;
  top: 50%;
  right: 0;
  z-index: 9;
  animation: zy 1.5s infinite 2s;
}



@keyframes zy {
  10% {
    transform: rotate(15deg);
  }
  20% {
    transform: rotate(-10deg);
  }
  30% {
    transform: rotate(5deg);
  }
  40% {
    transform: rotate(-5deg);
  }
  50%, 100% {
    transform: rotate(0deg);
  }
}
#blankOuter {
  width: 3rem;
  height: 4rem;
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  z-index: 10;
  color: #ccc;
  font-size: .32rem;
  text-align: center;
  line-height: .533333rem;

}
#blankOuter img{
  width: 3rem;
  height: 3rem;
}
