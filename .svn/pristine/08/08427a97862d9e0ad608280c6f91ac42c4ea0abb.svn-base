.welfare{
  margin-top: 0.3rem;
  width: 9.2rem;
  margin: auto;
  height: 7.66rem;
  -webkit-tap-highlight-color: transparent;
}
.welfare_img{
  height: 4.93rem;
  position: relative;
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.welfare_img p{
  height: 0.8rem;
  position: absolute;
  bottom: 0;
  background: rgba(0,0,0,0.6);
  line-height: 0.8rem;
  color: #fff;
  font-size: 0.25rem;
  width: 100%;
  padding-left: 2%;
}
.welfare_img .country{
  background: url('../../images/view/map.png') no-repeat center left #fe3657;
  background-size: 0.25rem 0.32rem;
  background-position-x: 0.3rem;
  padding: 0.14rem 0.14rem;
  color: #fff;
  font-size: 0.25rem;
  border-radius: 0 0.12rem 0.12rem 0;
  position: absolute;
  left: 0;
  padding-left: 0.78rem;
  top: 0.29rem;
}
.welfare_img .country .country_region{
  margin-left: -0.1rem;
}
.welfare_img .watch{
  background: url('../../images/view/liulan.png') no-repeat center left rgba(0,0,0,0.6);
  background-size: 0.32rem 0.18rem;
  background-position-x: 0.16rem;
  padding: 0.15rem 0.14rem;
  color: #fff;
  font-size: 0.25rem;
  border-radius: 0.12rem 0 0 0.12rem; 
  position: absolute;
  right: 0;
  padding-left: 0.6rem;
  padding-right: 0.2rem;
  top: 1.24rem;
}
.welfare_img .people{
  background: url('../../images/view/people.png') no-repeat center left rgba(0,0,0,0.6);
  background-size: 0.32rem 0.26rem;
  background-position-x: 0.16rem;
  padding: 0.15rem 0.14rem;
  color: #fff;
  font-size: 0.25rem;
  border-radius: 0.12rem 0 0 0.12rem;
  position: absolute;
  right: 0;
  padding-left: 0.6rem;
  padding-right: 0.2rem;
  top: 0.29rem;
}
.details{
  height: 2.4rem;
  box-shadow: 0 0 0.06rem #ccc;
  padding: 0.2rem;
}
.details  .details_title{
  font-size: 0.33rem;
  color: #333333;
  font-weight: 500;
  margin-top: 0.1rem;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.details .details_name{
  font-size: 0.25rem;
  color: #333333;
  margin-top: 0.1rem;
  height: 0.4rem;
}
.button {
  border-top: 1px dotted #eee;
  margin-top: 0.2rem;
}
.button .share{
  width: 2.13rem;
  height: 0.6rem;
  text-indent: 0.5rem;
  background: url('../../images/view/go_share.png') no-repeat center left;
  background-size: 2.13rem 0.6rem;
  text-align: center;
  color: #fff;
  text-indent: 0;
  font-size: 0.32rem;
  float: right;
  margin-top: 0.12rem;
  line-height: 0.6rem;
}
.button  span{
  font-size: 0.23rem;
  display: inline-block;
  text-align: center;
  line-height: 0.53rem;
  margin-top: 0.18rem;
}
.button  .star{
